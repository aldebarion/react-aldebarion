{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "globals": {
    "fetch": false,
    "firebase": false
  },
  "rules": {
    "semi": [2, "never"],
    "indent": ["error", 2],
    "arrow-body-style": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "react/forbid-prop-types": 0,
    "jsx-a11y/no-static-element-interactions": "off"
  }
}
