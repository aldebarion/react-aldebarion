{
  "name": "react-aldebarion",
  "version": "0.1.3",
  "description": "react implementation of the aldebarion guidelines",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development BUILD=examples webpack-dev-server",
    "start:dist": "npm run build && cross-env NODE_ENV=development LIB=from_dist webpack-dev-server",
    "start:examples:production": "npm run build && cross-env NODE_ENV=development LIB=from_dist webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --display-error-details",
    "build:examples": "cross-env NODE_ENV=production BUILD=examples webpack --display-error-details",
    "lint": "eslint --ext js,jsx src && find ./src/ -iname \"*.scss\" -exec sass-lint -v -q {} +",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldebarion/react-aldebarion.git"
  },
  "keywords": [
    "react",
    "aldebarion",
    "framework"
  ],
  "author": "Thibault Friedrich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldebarion/react-aldebarion/issues"
  },
  "homepage": "https://github.com/aldebarion/react-aldebarion#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.3",
    "babel-loader": "~7.1.1",
    "babel-plugin-syntax-class-properties": "~6.13.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "~6.23.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2017": "^6.14.0",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.24.1",
    "chai": "~4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "expect": "~1.20.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "~0.11.2",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.3.1",
    "jsdom": "^11.1.0",
    "mocha": "~3.5.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "~1.3.0",
    "sass-lint": "^1.8.2",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.0",
    "style-loader": "~0.18.2",
    "webpack": "^3.5.4",
    "webpack-dev-server": "~2.7.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "hoc-react-loader": "^6.1.0",
    "lodash": "^4.14.2",
    "muicss": "^0.9.20",
    "normalize-scss": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hexagon": "^1.1.1",
    "react-redux": "5.0.6",
    "react-router": "4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "~3.7.2",
    "redux-little-router": "^14.2.0",
    "redux-thunk": "~2.2.0",
    "reselect": "~3.0.1",
    "tinycolor2": "^1.4.1"
  }
}
